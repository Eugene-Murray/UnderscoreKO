<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>UnderscoreKO</id>
    <version>1.0.0</version>
    <authors>Kamran Ayub</authors>
    <description>Adds all the useful collection and array methods from Underscore.js to Knockout observable arrays. Also includes several convenience methods to maniuplate the underlying array.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/kamranayub/UnderscoreKO</projectUrl>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <dependencies>
      <dependency id="knockoutjs" version="2.0" />
      <dependency id="underscore.js" version="1.3" />
    </dependencies>
  </metadata>
  <files>
    <file src="underscore-ko.js" target="content" />
    <file src="underscore-ko.min.js" target="content" />
  </files>
</package>